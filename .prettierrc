{
  "semi": true,
  "singleQuote": false,
  "tabWidth": 2,
  "printWidth": 100,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "endOfLine": "lf",
  "plugins": ["@prettier/plugin-pug"],
  "overrides": [
    {
      "files": "*.pug",
      "options": {
        "parser": "pug"
      }
    }
  ]
}